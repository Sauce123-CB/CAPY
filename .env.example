# CAPY Environment Variables Template
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Machine identification
MACHINE_NAME=Mac
USER_NAME=YourName
OS_TYPE=mac

# Paths
CAPY_ROOT=~/Dev/CAPY
SOURCE_DOCS_PATH=~/Dropbox/CAPY-Sources

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# PDF Processing (Poppler)
# Install: brew install poppler
# Then: pip3 install pdfplumber pdf2image pillow
POPPLER_PATH=/opt/homebrew/bin

# Required versions (as of 2024-12-23):
# - poppler: 25.12.0+ (brew)
# - pdfplumber: 0.11.8+ (pip3)
# - pdf2image: 1.17.0+ (pip3)
# - pillow: 12.0.0+ (pip3)
